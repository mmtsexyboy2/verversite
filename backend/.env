DATABASE_URL="postgresql://user:password@postgres:5432/mydatabase"
GOOGLE_CLIENT_ID="YOUR_GOOGLE_CLIENT_ID"
GOOGLE_CLIENT_SECRET="YOUR_GOOGLE_CLIENT_SECRET"
JWT_SECRET="a_very_secret_key_that_should_be_long_and_random_min_32_chars_long"
PORT=3001
FRONTEND_URL="http://localhost:3000"

# Note: For Google OAuth to work, you'll need to replace
# YOUR_GOOGLE_CLIENT_ID and YOUR_GOOGLE_CLIENT_SECRET with actual credentials
# from a Google Cloud Project with OAuth 2.0 configured.
# The callback URL in Google Cloud project must match what passport-setup.js expects,
# typically http://localhost:3001/auth/google/callback for local development if backend is directly accessible,
# or the public URL if deployed. The `proxy: true` setting in passport helps if it's behind a reverse proxy.
